---
import { ViewTransitions } from "astro:transitions";

const preFetchList: string[] = ["/background-dark.png", "/background-light.png"];

---
<ViewTransitions />
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Alkatra&family=Inter:wght@300;400&family=Montserrat:wght@800&display=swap" rel="stylesheet">
{
    preFetchList.map((path) => {
        return <link rel="prefetch" href={path} />
    })
}
<meta name="renderDate" content={Date.now().toLocaleString()} />